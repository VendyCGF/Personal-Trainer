JAVA


/* :: Rotating Words
   ---------------------------------- */
   var animationDelay = 3500;
   var animating = true;

   function animateWords(wordWrap) {
     var duration = animationDelay; // Initial Animation In

     TweenMax.to($('.word-wrapper--active'), 0.2, {
       opacity: 1,
       ease: Power3.easeOut
     });
     $('.word-wrapper').css('width', $('.word-wrapper--active').width()); // Start the Loop

     setTimeout(function () {
       hideWord(wordWrap.find('.word-wrapper--active').eq(0));
     }, duration);
   } // :---- HIDE


   function hideWord(word) {
     if (animating === true) {
       var nextWord = takeNext(word);
       switchWord(word, nextWord);
       var animations = setTimeout(function () {
         hideWord(nextWord);
       }, animationDelay);
     }
   }

   function takeNext(word) {
     return !word.is(':last-child') ? word.next() : word.parent().children().eq(0);
   }

   function takePrev(word) {
     return !word.is(':first-child') ? word.prev() : word.parent().children().last();
   }

   function switchWord(oldWord, newWord) {
     if (animating === true) {
       // Set parent width to new word
       $('.word-wrapper').css('width', newWord.width()); // Old Word Tranistion

       oldWord.removeClass('word-wrapper--active');
       TweenMax.to(oldWord, 0.7, {
         opacity: 0,
         ease: Power1.easeOut
       }); // New Word Tranistion

       newWord.addClass('word-wrapper--active');
       var sliced = new SplitText(newWord, {
         type: 'words,chars'
       });
       TweenLite.set(newWord, {
         perspective: 100
       });
       TweenMax.set(newWord, {
         opacity: 1
       });
       TweenMax.staggerFrom(sliced.chars, 1.3, {
         opacity: 0,
         y: 64,
         rotationX: 0,
         ease: Elastic.easeOut
       }, 0.1);
     }
   }

   animateWords($('.word-wrapper')); // Window is Not in Focus -- pause animations

   window.addEventListener('blur', function () {
     animating = false;
   }, false); // Window is in Focus -- resume animations

   window.addEventListener('focus', function () {
     animating = true;
     animateWords($('.word-wrapper'));
   }, false);
 },
 finalize: function finalize() {}
});

/***/ }),
